<body>
  <header><h2>Welcome to TinyApp</h2>
  <h3 style='display: flex; align-items: center;'>
    <label for='register'>Register for an account:</label>
    <form name='register' method='post' action='/register' style='margin: 5px'>
      <input type='submit' value='Register'>
    </form>
  </header>
  <h3>Login</h3>
  <form method="post" action="/login">
    <label for='email'>Email:</label>
    <input type='email' name='email' placeholder='example@example.com' style='width:150px'>
    <label for='password'>Password:</label>
    <input type='password' name='password' placeholder='Password'>
    <input type='submit' value='Login'>
  </form>
  <h2>url Database</h2>
  <% Object.keys(urls).forEach(function (key) { %>
  <li style='display: flex; align-items: center;'>
    <a href='<%= urls[key].url %>'> <%= key %></a> â†’<%= urls[key].url %>
  </li>
  <% }); %>
</body>

